{
  "carolOrganization": "frontend",
  "carolEnvironment": "andredev",
  "/api/*": {
      "target": "https://frontend.carol.ai",
      "secure": true,
      "logLevel": "debug",
      "changeOrigin": true
  },
  "/sql/*": {
      "target": "https://api.carol.ai/sql/v1/query",
      "secure": true,
      "logLevel": "debug",
      "changeOrigin": true,
      "pathRewrite": {
          "^/sql": "/"
      }
  }
}